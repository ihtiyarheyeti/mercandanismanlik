{
  "name": "mercan-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "download-blog-images": "ts-node src/scripts/downloadBlogImages.ts"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "axios": "^1.7.9",
    "chart.js": "^4.4.8",
    "gsap": "^3.12.5",
    "html2canvas": "^1.4.1",
    "pinia": "^3.0.1",
    "three": "^0.161.0",
    "tsparticles": "^2.12.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-carousel": "^0.14.0",
    "vue3-particles": "^2.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.2"
  }
}
